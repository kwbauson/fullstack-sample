{
  "name": "fullstack-sample",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "./prepare.sh && concurrently -k yarn:start:*",
    "start:api-watch": "tsc --watch --preserveWatchOutput",
    "start:api-serve": "node-dev --respawn dist/api",
    "start:app": "parcel watch app/index.html",
    "start:db": "postgres -k \"$PGDATA\"",
    "build": "concurrently -k yarn:build:*",
    "build:api": "tsc --build",
    "build:app": "parcel build app/index.html"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "express": "4.17.1",
    "pg": "8.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "tslib": "2.3.0",
    "typeorm": "0.2.34"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "16.4.0",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/webpack-env": "1.16.2",
    "concurrently": "6.2.0",
    "node-dev": "7.0.0",
    "parcel": "2.0.0-beta.3.1",
    "typescript": "4.3.5"
  }
}
